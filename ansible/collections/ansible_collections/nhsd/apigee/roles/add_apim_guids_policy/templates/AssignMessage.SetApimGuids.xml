<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage continueOnError="false" enabled="true" name="{{ POLICY_NAME }}">
  <!-- This method of settings guids will have to be revisted -->
  <!-- when/if a single proxy handles multiple specs. -->
  <DisplayName>Set APIM Guids</DisplayName>
  <AssignVariable>
    <Name>apim.api_guid</Name>
    <Value>{{ API_GUID }}</Value>
  </AssignVariable>
  <AssignVariable>
    <Name>apim.api_spec_guid</Name>
    <Value>{{ API_SPEC_GUID }}</Value>
  </AssignVariable>
</AssignMessage>
